{
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "semi": false,
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": "*.njk",
      "options": {
        "parser": "jinja-template"
      }
    }
  ],
  "plugins": ["prettier-plugin-jinja-template"]
}
