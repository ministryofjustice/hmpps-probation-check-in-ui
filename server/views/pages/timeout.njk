{% extends "../layouts/base.njk" %}

{% block pageContent %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      <h1 class="govuk-heading-l">{{ content.title }}</h1>
      <p class="govuk-body">{{ content.message }}</p>
      {{
        govukButton({
          text: content.buttonText,
          href: "/" + submissionId
        })
      }}
    </div>
  </div>
{% endblock %}
